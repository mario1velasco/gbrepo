{
    "version": 2,
    "builds": [
        {
            "src": "dist/apps/unsplash/**",
            "use": "@vercel/static-build",
            "config": {
                "rewrites": [
                    {
                        "source": "/(.*)",
                        "destination": "/index.html"
                    }
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}